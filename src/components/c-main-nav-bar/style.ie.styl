@require "../../globals/css/_ietoolkit";

.c-main-nav-bar {
  position relative
  ul {
    display block
    border-left: 1px solid $border;
  }
  li {
    float:left;
    border-left: 0;
    border-right: 1px solid $border;
    ul {
      position relative
      li {
        float: none;
      }
    }
  }
}
